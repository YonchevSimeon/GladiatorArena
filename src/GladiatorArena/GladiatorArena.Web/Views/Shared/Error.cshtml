@using Microsoft.AspNetCore.Mvc.ModelBinding;
@model ModelStateDictionary

<div class="text-center bg-red m-5 p-10">
    @foreach (var error in Model)
    {
        foreach (var message in error.Value.Errors)
        {
            <h2 class="text-dark">@message.ErrorMessage</h2>
        }
    }
</div>
<div class="text-center p-10">
    <button onclick="window.history.back()" class="btn btn-primary" type="button">Back</button>
</div>